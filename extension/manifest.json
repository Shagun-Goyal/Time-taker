{
  "manifest_version": 3,
  "name": "TimeTracker",
  "version": "1.0.0",
  "description": "Tracks time spent on websites and provides productivity analytics",
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}